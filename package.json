        "Roboto Mono Light",
        "Roboto Mono Medium",
        "Roboto Mono Thin",
        "SK Modernist",
        "Share Tech Mono",
        "Source Code Pro",
        "Source Code Pro Extra Light",
        "Source Code Pro Light",
        "Source Code Pro Medium",
        "Space Mono",
        "Sudo",
        "TeX Gyre Cursor",
        "Terminus",
        "Ubuntu Mono",
        "Ubuntu Mono - Bront",
        "VT323",
        "Verily Serif Mono",
        "agave",
        "iA Writer Mono S",
        "saxMono"
      ]
    },
    "secondaryFonts": {
      "description": "Additional fonts for glyphs missing in the main font (e.g. for CJK characters).",
      "type": "string",
      "default": "monospace"
    }
  },
  "devDependencies": {
    "js-yaml": "^3.12.0"
  }
